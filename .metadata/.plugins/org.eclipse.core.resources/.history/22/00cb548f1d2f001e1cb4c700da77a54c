package empresa;

public class Principal {

	//métodos são analogos a funções
	public static void main(String[] args) {
		System.out.println("Criado a classe de aluno:\n\n");
		
		System.out.println("Objeto 1\n");
		Aluno objetoUmDeAluno = new Aluno();//instânciando um objeto de aluno
		objetoUmDeAluno.nome = "Jonas";
		objetoUmDeAluno.cpf = "01614346062";
		objetoUmDeAluno.matricula = 001;
		
		//Usando o método para exibir os atributos
		objetoUmDeAluno.info();
		
		System.out.println("Objeto 2\n");
		Aluno objetoDoisDeAluno = new Aluno();//instânciando um objeto de aluno
		objetoDoisDeAluno.nome = "Sabrina";
		objetoDoisDeAluno.cpf = "03635674028";
		objetoDoisDeAluno.matricula = 002;
		
		objetoDoisDeAluno.info();

	}

}
